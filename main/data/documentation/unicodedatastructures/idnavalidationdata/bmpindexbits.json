eeds"},{"type":"text","text":" "},{"type":"text","text":"to be binary-searched. However, the number of samples required to index N bits is "},{"type":"codeVoice","code":"(2^N) + 1"},{"type":"text","text":","},{"type":"text","text":" "},{"type":"text","text":"so it doubles in size for each additional bit we index."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"The default is to index 6 bits, giving 64 samples (130 bytes, since samples are 16 bits each)."},{"type":"text","text":" "},{"type":"text","text":"Each database may decide to index more or fewer bits."}],"type":"paragraph"}]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"kind":"symbol","sections":[],"references":{"doc://com.karwa.WebURL/documentation/UnicodeDataStructures/IDNAValidationData":{"identifier":"doc:\/\/com.karwa.WebURL\/documentation\/UnicodeDataStructures\/IDNAValidationData","title":"IDNAValidationData","url":"\/documentation\/unicodedatastructures\/idnavalidationdata","type":"topic","role":"symbol","abstract":[{"type":"text","text":"Data used to validate domain labels in IDNA."}],"fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"IDNAValidationData"}],"navigatorTitle":[{"kind":"identifier","text":"IDNAValidationData"}],"kind":"symbol"},"doc://com.karwa.WebURL/documentation/UnicodeDataStructures/IDNAValidationData/BMPIndexBits":{"type":"topic","kind":"symbol","identifier":"doc:\/\/com.karwa.WebURL\/documentation\/UnicodeDataStructures\/IDNAValidationData\/BMPIndexBits","fragments":[{"text":"static","kind":"keyword"},{"text":" ","kind":"text"},{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"BMPIndexBits","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Si","text":"Int","kind":"typeIdentifier"}],"title":"BMPIndexBits","role":"symbol","abstract":[{"text":"How many bits to index from BMP code-points.","type":"text"}],"url":"\/documentation\/unicodedatastructures\/idnavalidationdata\/bm